{
  "name": "directus-presenter",
  "version": "1.0.0",
  "description": "Lyrics Presenter Based On Directus",
  "scripts": {
    "start": "npm-run-all --parallel cmd-directus cmd-front open-browser",
    "cmd-directus": "npx directus start",
    "cmd-front": "npm run --prefix front dev",
    "open-browser": "wait-on http://localhost:8055 && open http://localhost:8055"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "open": "^9.1.0",
    "wait-on": "^7.0.1"
  }
}
